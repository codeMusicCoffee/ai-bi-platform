data: {
  "type": "artifact_file",
  "content": "{\"path\": \"/components/ModuleLoader.tsx\", \"code\": \"import React, { useState, useEffect } from 'react';\\nimport { COLORS } from '../utils/colors';\\nimport { BarChartCard } from './BarChartCard';\\nimport { Loader2, AlertCircle, Database } from 'lucide-react';\\n\\n// \\u6a21\\u5757\\u914d\\u7f6e\\u7c7b\\u578b\\ninterface ModuleConfig {\\n  module_config_id: string;\\n  module_name: string;\\n  description: string;\\n  chart_style: string;\\n  dataset_id: string;\\n  dataset_fields: string[];\\n  data?: any[];\\n}\\n\\n// API \\u54cd\\u5e94\\u7c7b\\u578b\\ninterface ApiResponse {\\n  data: any[];\\n}\\n\\ninterface ModuleLoaderProps {\\n  module: ModuleConfig;\\n  // \\u5982\\u679c\\u63d0\\u4f9b\\u4e86 data \\u5219\\u76f4\\u63a5\\u4f7f\\u7528\\uff0c\\u5426\\u5219\\u4ece API \\u83b7\\u53d6\\n  presetData?: any[];\\n}\\n\\nexport default function ModuleLoader({ module, presetData }: ModuleLoaderProps) {\\n  const [data, setData] = useState<any[]>([]);\\n  const [loading, setLoading] = useState(true);\\n  const [error, setError] = useState<string | null>(null);\\n\\n  // \\u83b7\\u53d6\\u5b57\\u6bb5\\u914d\\u7f6e\\n  const fields = module.dataset_fields || [];\\n  const xAxisKey = fields.find((f: string) => f !== 'unit_price' && f !== 'quantity') || 'brand';\\n  const dataKey = fields.find((f: string) => f === 'unit_price' || f === 'quantity') || fields[0];\\n\\n  useEffect(() => {\\n    const fetchData = async () => {\\n      // \\u5982\\u679c\\u6709\\u9884\\u8bbe\\u6570\\u636e\\uff0c\\u76f4\\u63a5\\u4f7f\\u7528\\n      if (presetData && presetData.length > 0) {\\n        setData(presetData);\\n        setLoading(false);\\n        return;\\n      }\\n\\n      // \\u5982\\u679c\\u6a21\\u5757\\u81ea\\u5e26\\u6570\\u636e\\uff0c\\u4f7f\\u7528\\u5b83\\n      if (module.data && module.data.length > 0) {\\n        setData(module.data);\\n        setLoading(false);\\n        return;\\n      }\\n\\n      // \\u5426\\u5219\\u5c1d\\u8bd5\\u4ece API \\u83b7\\u53d6\\n      try {\\n        const apiUrl = `//api/datasets/${module.dataset_id}/data`;\\n        const response = await fetch(apiUrl);\\n        \\n        if (!response.ok) {\\n          throw new Error(`HTTP error! status: ${response.status}`);\\n        }\\n        \\n        const json: ApiResponse = await response.json();\\n        const items = json?.data || [];\\n        \\n        // \\u53ea\\u4f7f\\u7528\\u914d\\u7f6e\\u7684\\u5b57\\u6bb5\\n        const filteredData = items.map((item: any) => {\\n          const filtered: any = {};\\n          fields.forEach((field: string) => {\\n            filtered[field] = item[field];\\n          });\\n          return filtered;\\n        });\\n        \\n        setData(filteredData);\\n        setError(null);\\n      } catch (err) {\\n        setError(err instanceof Error ? err.message : 'Failed to fetch data');\\n        // \\u4f7f\\u7528\\u6a21\\u5757\\u81ea\\u5e26\\u6570\\u636e\\u4f5c\\u4e3a\\u56de\\u9000\\n        if (module.data) {\\n          setData(module.data);\\n          setError(null);\\n        }\\n      } finally {\\n        setLoading(false);\\n      }\\n    };\\n\\n    fetchData();\\n  }, [module.dataset_id, module.data, presetData, fields]);\\n\\n  // \\u683c\\u5f0f\\u5316\\u663e\\u793a\\u503c\\n  const formatValue = (value: any): string => {\\n    if (typeof value === 'number') {\\n      if (dataKey === 'unit_price') {\\n        return `\\u00a5${value.toFixed(2)}`;\\n      }\\n      return value.toLocaleString();\\n    }\\n    return String(value ?? '');\\n  };\\n\\n  // \\u6e32\\u67d3\\u52a0\\u8f7d\\u72b6\\u6001\\n  if (loading) {\\n    return (\\n      <div\\n        className=\\\"rounded-lg p-6 shadow-md\\\"\\n        style={{\\n          backgroundColor: COLORS.surface,\\n          border: `1px solid ${COLORS.border}`,\\n          minHeight: '300px',\\n        }}\\n      >\\n        <h3\\n          className=\\\"text-lg font-semibold mb-4\\\"\\n          style={{ color: COLORS.text }}\\n        >\\n          {module.module_name}\\n        </h3>\\n        <div\\n          className=\\\"flex flex-col items-center justify-center rounded\\\"\\n          style={{\\n            height: '250px',\\n            backgroundColor: COLORS.secondaryLight,\\n          }}\\n        >\\n          <Loader2\\n            className=\\\"animate-spin mb-2\\\"\\n            size={32}\\n            style={{ color: COLORS.primary }}\\n          />\\n          <span style={{ color: COLORS.textMuted }}>\\u52a0\\u8f7d\\u4e2d...</span>\\n        </div>\\n      </div>\\n    );\\n  }\\n\\n  // \\u6e32\\u67d3\\u9519\\u8bef\\u72b6\\u6001\\n  if (error && (!data || data.length === 0)) {\\n    return (\\n      <div\\n        className=\\\"rounded-lg p-6 shadow-md\\\"\\n        style={{\\n          backgroundColor: COLORS.surface,\\n          border: `1px solid ${COLORS.border}`,\\n        }}\\n      >\\n        <h3\\n          className=\\\"text-lg font-semibold mb-4\\\"\\n          style={{ color: COLORS.text }}\\n        >\\n          {module.module_name}\\n        </h3>\\n        <div\\n          className=\\\"flex flex-col items-center justify-center rounded\\\"\\n          style={{\\n            height: '250px',\\n            backgroundColor: '#FEF2F2',\\n            border: `1px solid ${COLORS.error}`,\\n          }}\\n        >\\n          <AlertCircle size={32} style={{ color: COLORS.error }} className=\\\"mb-2\\\" />\\n          <span style={{ color: COLORS.error }}>\\u6570\\u636e\\u52a0\\u8f7d\\u5931\\u8d25</span>\\n          <span style={{ color: COLORS.textMuted, fontSize: '12px' }}>{error}</span>\\n        </div>\\n      </div>\\n    );\\n  }\\n\\n  // \\u6e32\\u67d3\\u7a7a\\u6570\\u636e\\u72b6\\u6001\\n  if (!data || data.length === 0) {\\n    return (\\n      <div\\n        className=\\\"rounded-lg p-6 shadow-md\\\"\\n        style={{\\n          backgroundColor: COLORS.surface,\\n          border: `1px solid ${COLORS.border}`,\\n        }}\\n      >\\n        <h3\\n          className=\\\"text-lg font-semibold mb-4\\\"\\n          style={{ color: COLORS.text }}\\n        >\\n          {module.module_name}\\n        </h3>\\n        <div\\n          className=\\\"flex flex-col items-center justify-center rounded\\\"\\n          style={{\\n            height: '250px',\\n            backgroundColor: COLORS.secondaryLight,\\n          }}\\n        >\\n          <Database size={32} style={{ color: COLORS.textMuted }} className=\\\"mb-2\\\" />\\n          <span style={{ color: COLORS.textMuted }}>\\u6682\\u65e0\\u6570\\u636e</span>\\n        </div>\\n      </div>\\n    );\\n  }\\n\\n  // \\u6839\\u636e\\u56fe\\u8868\\u7c7b\\u578b\\u6e32\\u67d3\\n  switch (module.chart_style) {\\n    case 'chart-bar':\\n    default:\\n      return (\\n        <BarChartCard\\n          title={module.module_name}\\n          data={data}\\n          dataKey={dataKey}\\n          xAxisKey={xAxisKey}\\n          color={COLORS.primary}\\n          height={300}\\n          showLegend={true}\\n          tooltipFormatter={formatValue}\\n          yAxisLabel={dataKey === 'unit_price' ? '\\u5355\\u4ef7(\\u5143)' : '\\u9500\\u552e\\u6570\\u91cf'}\\n        />\\n      );\\n  }\\n}\", \"status\": \"success\"}"
}

