data: {
  "type": "artifact_file",
  "content": "{\"path\": \"/components/Charts.tsx\", \"code\": \"import React from 'react';\\nimport { \\n  LineChart as ReLineChart, \\n  Line, \\n  XAxis, \\n  YAxis, \\n  CartesianGrid, \\n  Tooltip, \\n  ResponsiveContainer,\\n  PieChart as RePieChart,\\n  Pie,\\n  Cell,\\n  Legend\\n} from 'recharts';\\nimport { COLORS } from '../utils/colors';\\n\\ninterface ChartProps {\\n  data: any[];\\n  title: string;\\n}\\n\\nfunction CustomTooltip({ active, payload, label }: any) {\\n  if (active && payload && payload.length) {\\n    return (\\n      <div className=\\\"p-3 bg-white border border-slate-200 shadow-lg rounded-lg\\\">\\n        <p className=\\\"text-sm font-bold text-slate-800 mb-1\\\">{label}</p>\\n        {payload.map((entry: any, index: number) => (\\n          <p key={index} className=\\\"text-xs font-medium\\\" style={{ color: entry.color }}>\\n            {entry.name}: {entry.value.toLocaleString()}\\n          </p>\\n        ))}\\n      </div>\\n    );\\n  }\\n  return null;\\n}\\n\\nexport function SimpleLineChart({ data = [], title }: ChartProps) {\\n  return (\\n    <div className=\\\"bg-white p-6 rounded-xl border border-slate-100 shadow-sm h-[350px] w-full\\\">\\n      <h3 className=\\\"text-lg font-semibold mb-6\\\" style={{ color: COLORS.text }}>{title}</h3>\\n      <div className=\\\"h-[250px] w-full\\\">\\n        <ResponsiveContainer width=\\\"100%\\\" height=\\\"100%\\\">\\n          <ReLineChart data={data} margin={{ top: 5, right: 5, left: 0, bottom: 5 }}>\\n            <CartesianGrid strokeDasharray=\\\"3 3\\\" vertical={false} stroke={COLORS.border} />\\n            <XAxis \\n              dataKey=\\\"name\\\" \\n              axisLine={false} \\n              tickLine={false} \\n              tick={{ fill: COLORS.textMuted, fontSize: 12 }}\\n              dy={10}\\n            />\\n            <YAxis \\n              axisLine={false} \\n              tickLine={false} \\n              tick={{ fill: COLORS.textMuted, fontSize: 12 }}\\n            />\\n            <Tooltip content={<CustomTooltip />} />\\n            <Line \\n              type=\\\"monotone\\\" \\n              dataKey=\\\"value\\\" \\n              stroke={COLORS.primary} \\n              strokeWidth={3}\\n              dot={{ r: 4, fill: COLORS.primary, strokeWidth: 2, stroke: '#fff' }}\\n              activeDot={{ r: 6, strokeWidth: 0 }}\\n            />\\n          </ReLineChart>\\n        </ResponsiveContainer>\\n      </div>\\n    </div>\\n  );\\n}\\n\\nexport function SimplePieChart({ data = [], title }: ChartProps) {\\n  return (\\n    <div className=\\\"bg-white p-6 rounded-xl border border-slate-100 shadow-sm h-[350px] w-full\\\">\\n      <h3 className=\\\"text-lg font-semibold mb-2\\\" style={{ color: COLORS.text }}>{title}</h3>\\n      <div className=\\\"h-[260px] w-full\\\">\\n        <ResponsiveContainer width=\\\"100%\\\" height=\\\"100%\\\">\\n          <RePieChart>\\n            <Pie\\n              data={data}\\n              cx=\\\"50%\\\"\\n              cy=\\\"50%\\\"\\n              innerRadius={60}\\n              outerRadius={80}\\n              paddingAngle={5}\\n              dataKey=\\\"value\\\"\\n            >\\n              {(data || []).map((entry, index) => (\\n                <Cell key={`cell-${index}`} fill={COLORS.chart[index % COLORS.chart.length]} />\\n              ))}\\n            </Pie>\\n            <Tooltip content={<CustomTooltip />} />\\n            <Legend \\n              verticalAlign=\\\"bottom\\\" \\n              height={36}\\n              iconType=\\\"circle\\\"\\n              formatter={(value) => <span className=\\\"text-xs font-medium text-slate-600\\\">{value}</span>}\\n            />\\n          </RePieChart>\\n        </ResponsiveContainer>\\n      </div>\\n    </div>\\n  );\\n}\", \"status\": \"success\"}"
}

